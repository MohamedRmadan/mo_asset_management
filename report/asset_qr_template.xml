<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template id="report_asset_qr">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <t t-foreach="docs" t-as="o">
                        <div class="page">
                            <img t-att-src="'/report/barcode/QR/
                            '+o.asset_qr_code+''"/>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>