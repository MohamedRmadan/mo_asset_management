<odoo>
    <data noupdate="1">
        <record id="sequence_asset_loan" model="ir.sequence">
            <field name="name">Asset Loan</field>
            <field name="code">asset.loan</field>
            <field name="prefix">LON/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_asset" model="ir.sequence">
            <field name="name">Asset</field>
            <field name="code">asset</field>
            <field name="prefix">AST/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>